{
	"name": "@marsidev/web",
	"description": "Personal portfolio website of Luis Marsiglia.",
	"author": "Luis Marsiglia <marsiglia.business@gmail.com>",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 8000",
		"build": "tsc && next build",
		"start": "next start -p $PORT",
		"start:local": "next start -p 3000",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"tsc": "tsc",
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
	},
	"dependencies": {
		"@chakra-ui/button": "2.0.11",
		"@chakra-ui/form-control": "2.0.11",
		"@chakra-ui/input": "2.0.11",
		"@chakra-ui/layout": "2.1.8",
		"@chakra-ui/media-query": "3.2.7",
		"@chakra-ui/menu": "2.1.2",
		"@chakra-ui/provider": "2.0.19",
		"@chakra-ui/skeleton": "2.0.17",
		"@chakra-ui/system": "2.3.0",
		"@chakra-ui/textarea": "2.0.12",
		"@chakra-ui/theme-tools": "2.0.12",
		"@chakra-ui/theme-utils": "2.0.1",
		"@cloudinary/react": "1.4.2",
		"@cloudinary/url-gen": "1.8.6",
		"@emotion/react": "11.10.4",
		"@emotion/styled": "11.10.4",
		"@fontsource/nunito": "4.5.10",
		"@fontsource/rubik": "4.5.11",
		"@formkit/auto-animate": "1.0.0-beta.3",
		"@hookform/resolvers": "2.9.7",
		"@marsidev/react-hooks": "1.2.3",
		"@marsidev/react-turnstile": "^0.0.3",
		"@splitbee/web": "0.3.0",
		"atropos": "1.0.2",
		"framer-motion": "7.2.0",
		"jotai": "1.8.3",
		"next": "12.3.0",
		"next-mdx-remote": "4.1.0",
		"next-pwa": "5.6.0",
		"next-seo": "5.5.0",
		"react": "18.2.0",
		"react-div-100vh": "0.7.0",
		"react-dom": "18.2.0",
		"react-hook-form": "7.35.0",
		"react-icons": "4.4.0",
		"react-intersection-observer": "9.4.0",
		"react-toastify": "9.0.8",
		"zod": "3.19.0"
	},
	"devDependencies": {
		"@babel/core": "7.18.13",
		"@chakra-ui/theme": "2.1.14",
		"@cloudinary/html": "1.4.2",
		"@next/bundle-analyzer": "12.3.0",
		"@types/node": "18.7.16",
		"@types/react": "18.0.18",
		"@types/react-dom": "18.0.6",
		"@typescript-eslint/eslint-plugin": "5.36.2",
		"@typescript-eslint/parser": "5.36.2",
		"cross-env": "7.0.3",
		"eslint": "8.23.0",
		"eslint-config-marsi": "1.1.1",
		"eslint-config-next": "12.3.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard": "17.0.0",
		"eslint-import-resolver-typescript": "3.5.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-n": "15.2.5",
		"eslint-plugin-promise": "6.0.1",
		"eslint-plugin-react": "7.31.7",
		"typescript": "4.8.3"
	},
	"packageManager": "pnpm@7.12.1",
	"engines": {
		"node": "16.x"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "18",
				"react-dom": "18"
			},
			"ignoreMissing": [
				"webpack",
				"@babel/preset-env",
				"@chakra-ui/utils",
				"@chakra-ui/styled-system"
			]
		}
	},
	"keywords": [
		"portfolio",
		"website",
		"react",
		"typescript",
		"developer",
		"frontend"
	],
	"type": "module"
}
