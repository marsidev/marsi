		"atropos": "2.0.2",
		"framer-motion": "11.3.29",
		"jotai": "2.9.3",
		"next": "14.2.5",
		"next-mdx-remote": "4.4.1",
		"next-pwa": "5.6.0",
		"next-seo": "6.5.0",
		"react": "18.3.1",
		"react-div-100vh": "0.7.0",
