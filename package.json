		"@types/react": "18.3.4",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "6.19.1",
		"@typescript-eslint/parser": "6.19.1",
		"cross-env": "7.0.3",
		"eslint": "8.56.0",
		"eslint-config-marsi": "1.2.1",
		"eslint-config-next": "14.1.0",
		"eslint-config-prettier": "8.8.0",
