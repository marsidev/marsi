				"react": "18",
				"react-dom": "18"
			},
			"ignoreMissing": [
				"webpack",
				"@babel/preset-env",
				"@chakra-ui/utils",
				"@chakra-ui/styled-system"
			]
		}
	},
	"keywords": [
		"portfolio",
		"website",
		"react",
		"typescript",
		"developer",
		"frontend"
	],
	"type": "module"
